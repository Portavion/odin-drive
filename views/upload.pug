extends layout

block content
  h1 Please log-in
  if errors
    each error, i in errors
      div(class='alert alert-danger') #{error.msg}

  form(action="/file/upload" method="POST" enctype="multipart/form-data") 
      input(type="file" name="file" id="file" required)
      br.spacer
      button(type='submit') Submit

